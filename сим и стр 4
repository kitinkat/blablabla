int main() {
    int N;
    printf("Введите целое число N (1 ≤ N ≤ 26): ");
    scanf("%d", &N);

    if (N >= 1 && N <= 26) {
        printf("Первые %d прописных букв латинского алфавита:\n", N);
        for (int i = 0; i < N; i++) {
            printf("%c ", 'A' + i);
        }
        printf("\n");
    } else {
        printf("Ошибка: число должно быть в диапазоне 1-26\n");
    }
    
    return 0;
}
