int main() {
    char C;
    printf("Введите символ C: ");
    scanf("%c", &C);
  
    printf("Символ C: '%c' (код %d)\n", C, C);
  
    if (C > 0) {
        printf("Предыдущий: '%c' (код %d)\n", C - 1, C - 1);
    } else {
        printf("Предыдущий: за пределами таблицы\n");
    }
    
    if (C < 127) {
        printf("Следующий:  '%c' (код %d)\n", C + 1, C + 1);
    } else {
        printf("Следующий: за пределами таблицы\n");
    }
    
    return 0;
}
